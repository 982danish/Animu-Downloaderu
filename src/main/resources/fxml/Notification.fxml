<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox prefHeight="50.0" prefWidth="200.0" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.codingotaku.apps.custom.NotificationController">
   <VBox prefHeight="50.0" HBox.hgrow="ALWAYS">
      <children>
      	<Label fx:id="messageType" />
      	<Label fx:id="message" />
      </children>
   </VBox>
   <Button mnemonicParsing="false" onAction="#close" text="X" />
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
